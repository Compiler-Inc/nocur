{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "dialog:allow-open",
    "shell:allow-open",
    "os:default",
    "pty:default"
  ]
}
